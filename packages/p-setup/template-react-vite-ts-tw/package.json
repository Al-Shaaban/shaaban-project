{
  "name": "template-react-vite-ts-tw",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type:check": "tsc --noEmit",
    "lint": "npx @biomejs/biome lint --apply ./src",
    "format": "prettier --write ./src/**/*.{ts,tsx,css}",
    "vet": "npm run type:check && npm run lint && npm run format"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  }
}
